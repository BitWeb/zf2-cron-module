{
    "name": "bitweb/zf2-cron-module",
    "description": "BitWeb ZF2 module for cron.",
    "license": "BSD-3-Clause",
    "homepage": "http://bitweb.ee/",
    "authors": [
        {
            "name": "Kristjan Andresson",
            "email": "kristjan.andresson@bitweb.ee"
        },
        {
            "name": "Rain Ramm",
            "email": "rain@bitweb.ee"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "cron/cron": "1.0.0",
        "bitweb/stdlib": "~1.1",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-console": "~2.2",
        "zendframework/zend-servicemanager": "~2.2",
        "zendframework/zend-stdlib": "~2.2",
        "satooshi/php-coveralls": "dev-master",
        "sebastianbergmann/phpcov": "1.1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "": ["src/", "test/"]
        }
    }
}
